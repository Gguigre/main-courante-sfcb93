    "expo-constants": "~12.1.2",
    "expo-firebase-analytics": "~5.0.3",
    "expo-font": "~10.0.3",
    "expo-linking": "~2.4.2",
    "expo-localization": "~11.0.0",
    "expo-splash-screen": "~0.13.3",
